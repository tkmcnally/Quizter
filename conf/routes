# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

#Testing restFB android
POST	/api/android/facebook/connect	    controllers.Application.connectUser()

#Testing restFB android
POST	/api/android/facebook/questions	    controllers.Application.updateQuestions()

#Testing restFB android
POST	/api/android/facebook/loadquestions	    controllers.Application.loadQuestions()

POST 	/api/android/facebook/updatescore	controllers.Application.updateScore()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
