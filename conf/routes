# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

#Register page
POST	/api/register				controllers.Application.register()

#Login page
POST	/api/login					controllers.Application.authenticate()

#Testing restFB
GET		/api/facebook/:ACCESS_TOKEN	controllers.Application.htmlFacebookTest(ACCESS_TOKEN: String)

#Testing restFB android
POST	/api/android/facebook/	    controllers.Application.androidFacebookTest()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
